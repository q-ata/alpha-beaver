{
  "name": "api",
  "sub": [
    {
      "name": "users",
      "sub": [
        {
          "name": "userID",
          "path": ":userID",
          "sub": [
            {
              "name": "get",
              "query": {
                "method": "get",
                "auth": true,
                "dest": "api/authenticated/User"
              }
            }
          ]
        },
        {
          "name": "me",
          "sub": [
            {
              "name": "classes",
              "query": {
                "method": "get",
                "auth": true,
                "dest": "api/authenticated/me/Classes"
              }
            },
            {
              "name": "announcements",
              "query": {
                "method": "get",
                "auth": true,
                "dest": "api/authenticated/me/Announcements"
              }
            },
            {
              "name": "events",
              "query": {
                "method": "get",
                "auth": true,
                "dest": "api/authenticated/me/Events"
              }
            }
          ],
          "query": {
            "method": "get",
            "auth": true,
            "dest": "api/authenticated/me/Me"
          }
        }
      ]
    },
    {
      "name": "classes",
      "sub": [
        {
          "name": "classID",
          "path": ":classID",
          "sub": [
            {
              "name": "get",
              "query": {
                "method": "get",
                "auth": true,
                "dest": "api/authenticated/classes/Classes"
              }
            },
            {
              "name": "announcements",
              "query": {
                "method": "get",
                "auth": true,
                "dest": "api/authenticated/classes/Announcements"
              }
            },
            {
              "name": "addannouncement",
              "query": {
                "method": "post",
                "auth": true,
                "body": {
                  "title": "string",
                  "content": "string",
                  "date": "number"
                },
                "dest": "api/authenticated/classes/AddAnnouncement"
              }
            },
            {
              "name": "addevent",
              "query": {
                "method": "post",
                "auth": true,
                "body": {
                  "title": "string",
                  "content": "string",
                  "start": "number",
                  "end": "number"
                },
                "dest": "api/authenticated/classes/AddEvent"
              }
            },
            {
              "name": "addmodule",
              "query": {
                "method": "post",
                "auth": true,
                "body": {
                  "type": "integer",
                  "data": "object"
                },
                "dest": "api/authenticated/classes/AddModule"
              }
            },
            {
              "name": "setmodules",
              "query": {
                "method": "post",
                "auth": true,
                "body": {
                  "modules": [
                    {
                      "id": "integer",
                      "type": "integer",
                      "data": "object"
                    }
                  ]
                },
                "dest": "api/authenticated/classes/SetModule"
              }
            },
            {
              "name": "modules",
              "query": {
                "method": "get",
                "auth": true,
                "dest": "api/authenticated/classes/Module"
              }
            },
            {
              "name": "pages",
              "query": {
                "method": "get",
                "auth": true,
                "dest": "api/authenticated/classes/pages/Pages"
              },
              "sub": [
                {
                  "name": "pageID",
                  "path": ":pageID",
                  "sub": [
                    {
                      "name": "setmodules",
                      "query": {
                        "method": "post",
                        "auth": true,
                        "body": {
                          "modules": ["object"]
                        },
                        "dest": "api/authenticated/classes/pages/SetPageModules"
                      }
                    },
                    {
                      "name": "get",
                      "query": {
                        "method": "get",
                        "auth": true,
                        "dest": "api/authenticated/classes/pages/Page"
                      }
                    },
                    {
                      "name": "modules",
                      "query": {
                        "method": "get",
                        "auth": true,
                        "dest": "api/authenticated/classes/pages/Modules"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "name": "login",
      "query": {
        "method": "post",
        "body": {
          "username": "string",
          "password": "string",
          "school": "string"
        },
        "dest": "api/public/Login"
      }
    },
    {
      "name": "refresh",
      "query": {
        "method": "post",
        "body": {
          "user": "number",
          "school": "string"
        },
        "dest": "api/public/Refresh"
      }
    },
    {
      "name": "register",
      "query": {
        "method": "post",
        "body": {
          "username" : "string",
          "password": "string",
          "school": "string"
        },
        "dest": "api/public/Register"
      }
    },
    {
      "name": "schools",
      "query": {
        "method": "get",
        "dest": "api/public/Schools"
      }
    }
  ]
}